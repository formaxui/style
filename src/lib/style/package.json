    "style",
    "interface",
    "css-in-js",
    "typescript",
    "javascript",
    "react",
    "moraj"
  ],
  "bugs": {
    "url": "https://github.com/formaxui/utils/issues"
  },
  "homepage": "https://github.com/formaxui/utils#readme"
